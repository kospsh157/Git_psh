/*Reset*/
* { margin:0;	padding:0; }
html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, 
abbr, address, big, cite, code, del, dfn, em, font, img, ins,q, s, samp, small, strike, 
strong, sub, sup, tt, var, b,u, i, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, 
table, caption, tbody, tfoot, thead, tr, th, td { margin: 0; padding: 0; border: none 0; }
* { font-size:12px; font-family:'nanum gothic', sans-serif; color:#505050;  }
html body{height:100%; width:100%; background-color:#f7f7f7; }

textarea { overflow:auto; }
select, input, img, li { vertical-align: middle; text-align:center; outline:none;}
address, caption, cite, code, dfn, em, strong, th, var { font-style : normal; font-weight : normal; }
ul, li, ol { list-style-type: none;  }
/* ul{ font-size:0px; }크롬이 차이가 나서 0px를 주고 가야한다 */

legend,  caption { display: none; }
hr { border-collapse:collapse; display:none; }




a, a:link, a:visited, a:hover {	text-decoration:none;}


